<div class ="hero-unit">
  <h1>Listing users</h1>
  <br /><br />

  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th></th>
      <th></th>

    </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show', user %></td>
    </tr>
  <% end %>

  </table>

  <br />
</div>

